/shadeagrid {
  /gridvals exch def
  /bgcolor exch def
  /colormap exch def
<<
    /ShadingType 4
    /ColorSpace [/DeviceRGB]
%    /VerticesPerRow 2
    /Background bgcolor
    /Function colormap
    /DataSource gridvals
>>
shfill

} bind def

/redblue      <<
  /FunctionType 0
    /Domain [0 1]
    /Range [0 1 0 1 0 1]
    /Size [5]
    /BitsPerSample 8
    /DataSource <400000 ff0000 ffffff 0000ff 000040>
 >> def

/blue      <<
  /FunctionType 0
    /Domain [0 1]
    /Range [0 1 0 1 0 1]
    /Size [5]
    /BitsPerSample 8
    /DataSource <ffffff 8080ff 0000ff 000080 000040>
 >> def

/green      <<
  /FunctionType 0
    /Domain [0 1]
    /Range [0 1 0 1 0 1]
    /Size [5]
    /BitsPerSample 8
    /DataSource <ffffff 80ff80 00ff00 006000 002000>
 >> def


/gridvals [
      0 0 0 0
      0 100  0 .3
      0 0 100 1
      1 100 100 .7
      2 200 0 .35
      1 200 100 .5

      0 100 100 .7
      0 200 100  .8
      0 100 200 .65
      1 200 200 .85
      2 300 100 .4
      1 300 200 .25
] def

%green [1 1 1] gridvals shadeagrid
%blue [1 1 1] gridvals shadeagrid
redblue [1 1 1] gridvals shadeagrid
showpage
