/cube {
  5 dict begin
    /size exch def
    size 0 size size rectstroke
    size size size size rectstroke
    size 2 size mul size size rectstroke
    size 3 size mul size size rectstroke
    0 0 size size rectstroke
    size 2 mul 0 size size rectstroke
  end
} bind def

/pentagon {
    size 0 moveto
    size p1 cos mul size p1 sin mul lineto
    size p2 cos mul size p2 sin mul lineto
    size p3 cos mul size p3 sin mul lineto
    size p4 cos mul size p4 sin mul lineto
    closepath
    stroke
} bind def

/pentagoncluster {
    gsave
      p1 0.5 mul rotate
      shift 0 translate
      size pentagon
    grestore
    gsave
      p1 1.5 mul rotate
      shift 0 translate
      size pentagon
    grestore
    gsave
      p1 2.5 mul rotate
      shift 0 translate
      size pentagon
    grestore
    gsave
      p1 3.5 mul rotate
      shift 0 translate
      size pentagon
    grestore
    gsave
      p1 4.5 mul rotate
      shift 0 translate
      size pentagon
    grestore
} bind def
/dodecahedron {
  5 dict begin
    /size exch def
    /p1 360 5 div def
    /p2 p1 2 mul def
    /p3 p1 3 mul def
    /p4 p1 4 mul def
    /cs p1 cos def
    /sn p1 sin def
    /edge size p1 sin mul def
    /perp size size mul edge .5 mul dup mul sub sqrt def
    size pentagon
    /shift perp 1.85 mul def
    /shift2 shift 2.62 mul def
    p1 p2 add 2 div
    pentagoncluster
     p1 rotate
    shift2 0 translate
    180 rotate
    pentagoncluster
  end
} bind def

%190 cube
250  220 translate
85 dodecahedron

showpage