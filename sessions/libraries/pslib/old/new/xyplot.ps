/xyplot {
  /y exch def
  /x exch def
  x 0 get y 0 get moveto
  0 1 x length 1 sub {
    dup x exch get exch y exch get lineto
  } for
  stroke
} def

/scatter {
  /y exch def
  /x exch def
  /shape exch def
  0 1 x length 1 sub {
    dup x exch get exch y exch get
    gsave
%      translate /shape exec %-1 -1 2 2 rectfill
      translate -1 -1 2 2 rectfill
    grestore
  } for
} def

0 0 0 setrgbcolor
0 setlinewidth
/x [0  22 31 50 85 102 155 190 222] def
/y [50 60 56 65 81 80  58   26  51] def

x y xyplot
0 0 1 setrgbcolor
{ -1 -1 2 2 rectfill } x y scatter
1 0 0 setrgbcolor
{ -5 0 moveto 5 0 lineto stroke 0 -5 moveto 0 5 lineto stroke } x y scatter


0 300 translate
gsave
100 0 translate
newpath
-10 -10 moveto
20 0 rlineto
-10 15 rlineto
closepath
fill
grestore

gsave
200 0 translate
newpath
0 0 5 0 360 arc
fill
grestore

gsave
250 0 translate
0 0 10 10 rectfill
fill
grestore

gsave
300 0 translate
0 0 moveto (+) show
grestore


0 .7 0 setrgbcolor
200 250 moveto
/Symbol findfont 20 scalefont setfont
(*!+.ABC) show
(abcdefghijklmnopqrstuvwxyz) show
200 270 moveto
(ABCDEFGHIJKLMNOPQRSTUVWXYZ) show

